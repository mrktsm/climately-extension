{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Description of my extension.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["bundle.js", "index.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["activeTab"]
}
